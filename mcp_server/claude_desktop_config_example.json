{
  "mcpServers": {
    "paddleocr-local": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server"
    },
    "paddleocr-aistudio-ocr": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_MCP_OCR_SOURCE": "aistudio",
        "PADDLEOCR_MCP_SERVER_URL": "https://xxx.aistudio-hub.baidu.com/ocr",
        "AISTUDIO_ACCESS_TOKEN": "your_api_token_here"
      }
    },
    "paddleocr-aistudio-structure": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_MCP_OCR_SOURCE": "aistudio",
        "PADDLEOCR_MCP_SERVER_URL": "https://xxx.aistudio-hub.baidu.com/layout-parsing",
        "AISTUDIO_ACCESS_TOKEN": "your_api_token_here",
        "PADDLEOCR_MCP_TIMEOUT": "60"
      }
    },
    "paddleocr-user-service-ocr": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_MCP_OCR_SOURCE": "self_hosted",
        "PADDLEOCR_MCP_SERVER_URL": "your_server_url_here"
      }
    },
    "paddleocr-user-service-structure": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_MCP_OCR_SOURCE": "self_hosted",
        "PADDLEOCR_MCP_SERVER_URL": "your_server_url_here"
      }
    }
  }
} 
