{
  "mcpServers": {
    "paddleocr-local": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server"
    },
    "paddleocr-aistudio-ocr": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_ENGINE": "aistudio",
        "PADDLEOCR_API_URL": "https://xxx.aistudio-hub.baidu.com/ocr",
        "PADDLEOCR_API_TOKEN": "your_api_token_here"
      }
    },
    "paddleocr-aistudio-structure": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_ENGINE": "aistudio",
        "PADDLEOCR_API_URL": "https://xxx.aistudio-hub.baidu.com/layout-parsing",
        "PADDLEOCR_API_TOKEN": "your_api_token_here",
        "PADDLEOCR_TIMEOUT": "60"
      }
    },
    "paddleocr-local-service-ocr": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_ENGINE": "local_service",
        "PADDLEOCR_API_URL": "your_api_url_here"
      }
    },
    "paddleocr-local-service-structure": {
      "command": "python",
      "args": [
        "/absolute/path/to/PaddleOCR/mcp_server/main.py"
      ],
      "cwd": "/absolute/path/to/PaddleOCR/mcp_server",
      "env": {
        "PADDLEOCR_ENGINE": "local_service",
        "PADDLEOCR_API_URL": "your_api_url_here"
      }
    }
  }
} 